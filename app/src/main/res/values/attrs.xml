<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Indicator">
        <attr name="dotRadius" format="dimension|reference"/>
        <attr name="dotMargin" format="dimension|reference" />
        <attr name="focusedDotLength" format="dimension|reference" />
        <attr name="focusedDotColor" format="color|reference" />
        <attr name="unfocusedDotColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="MyFlowLayout">
        <attr name="lineSpacing" format="dimension|reference" />"
        <attr name="itemSpacing" format="dimension|reference" />"
        <attr name="constrainChildHeight" format="boolean" />
        <attr name="overflowBehavior" format="enum">
            <enum name="keep" value="0" />
            <enum name="truncate" value="1" />
            <enum name="skip" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyFlowLayout_LayoutParams">
        <attr name="android:layout_gravity" />/>
    </declare-styleable>

</resources>